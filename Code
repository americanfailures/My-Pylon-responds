discord.on(discord.Event.MESSAGE_CREATE, async (payload) => {
  switch (payload.content.toLowerCase()) {
    case 'whats your name':
      return await payload.reply(
        'insert the name you want'
      );
    case 'how are you':
      return await payload.reply('Good');
  }
});
